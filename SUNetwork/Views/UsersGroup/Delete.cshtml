@model SUNetwork.Models.USERS_GROUP

@{
    ViewBag.Title = "Sil";
}

<h3>Silmek istediğinizden emin misiniz?</h3>
<div>
      <hr />
    <dl class="dl-horizontal">
        <dt>
           Grup Adı
        </dt>

        <dd>
            @Html.DisplayFor(model => model.GROUP_NAME)
        </dd>

        <dt>
           Oluşturma Tarihi
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CREATE_DATE)
        </dd>

        <dt>
            Kullanıcı Id
        </dt>

        <dd>
            @Html.DisplayFor(model => model.USER_ID)
        </dd>

     </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Sil" class="btn btn-default" /> |
            @Html.ActionLink("Listeye Dön", "Index")
        </div>
    }
</div>
